            <action method="addJs"><script>aitoc/lnp/promise.js</script></action>
            <action method="addJs"><script>aitoc/lnp/prototype.js</script></action>
        
            <action method="addJs"><script>aitoc/lnp/Lnp.js</script></action>
            <action method="addJs"><script>aitoc/lnp/Core/Cookie.js</script></action>
            <action method="addJs"><script>aitoc/lnp/Core/App.js</script></action>
            <action method="addJs"><script>aitoc/lnp/Core/Component/Abstract.js</script></action>
